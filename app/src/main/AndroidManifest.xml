<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hello">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hello">
        <activity android:name=".architecture.ArchActivity"></activity>
        <activity android:name=".network.NetworkActivity" />
        <activity android:name=".room.ViewActivity" />
        <activity android:name=".room.SubmitActivity" />
        <activity android:name=".sharedPreference.SPActivity" />
        <activity android:name=".kotlinCoroutines.RxActivity" />
        <activity android:name=".recyclerView.RecyclerListActivity" />
        <activity android:name=".recyclerView.RecyclerViewActivity" />
        <activity android:name=".fragment.FragmentActivity" />
        <activity android:name=".LifecycleActivity" />
        <activity android:name=".ContactContentActivity">
            <intent-filter>
                <action android:name="android.intent.action.content" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ContactPersonActivity" />
        <activity android:name=".ActivityPracticesActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ConstraintActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>